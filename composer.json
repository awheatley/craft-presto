{
    "name": "lewiscom/presto",
    "description": "A static cache plugin for Craft CMS 3.x",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "presto"
    ],
    "support": {
        "docs": "https://github.com/lewiscom/craft-presto/presto/blob/master/README.md",
        "issues": "https://github.com/lewiscom/craft-presto/presto/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lewis Communications",
            "homepage": "https://www.lewiscommunications.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "alc/sitemap-crawler": "^1.0"
    },
    "autoload": {
        "psr-4": {
          "lewiscom\\presto\\": "src/"
        }
    },
    "extra": {
        "name": "Presto",
        "handle": "presto",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/lewiscom/craft-presto/presto/master/CHANGELOG.md",
        "components": {
            "prestoService": "lewiscom\\presto\\services\\PrestoService",
            "cacheService": "lewiscom\\presto\\services\\CacheService"
        },
        "class": "lewiscom\\presto\\Presto"
    }
}
